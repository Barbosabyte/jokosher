	    <packing>
	      <property name="padding">3</property>
	      <property name="expand">False</property>
	      <property name="fill">True</property>
	    </packing>
	  </child>

	  <child>
	    <widget class="GtkButton" id="setButton">
	      <property name="visible">True</property>
	      <property name="tooltip" translatable="yes">Sets the tempo value for the current project from the bpm currently displayed</property>
	      <property name="can_focus">True</property>
	      <property name="label" translatable="yes">_Set</property>
	      <property name="use_underline">True</property>
	      <property name="relief">GTK_RELIEF_NORMAL</property>
	      <property name="focus_on_click">False</property>
	      <signal name="clicked" handler="OnSetClicked" last_modification_time="Tue, 19 Dec 2006 20:27:07 GMT"/>
