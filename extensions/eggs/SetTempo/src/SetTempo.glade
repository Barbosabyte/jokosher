	      <property name="can_focus">True</property>
	      <property name="label" translatable="yes">Clea_r</property>
	      <property name="use_underline">True</property>
	      <property name="relief">GTK_RELIEF_NORMAL</property>
	      <property name="focus_on_click">False</property>
	      <signal name="clicked" handler="OnClearClicked" last_modification_time="Tue, 19 Dec 2006 20:26:49 GMT"/>
	    </widget>
	    <packing>
	      <property name="padding">3</property>
	      <property name="expand">False</property>
	      <property name="fill">True</property>
	    </packing>
	  </child>

	  <child>
	    <widget class="GtkButton" id="stopButton">
	      <property name="visible">True</property>
	      <property name="tooltip" translatable="yes">Press to stop the currently playing audio</property>
	      <property name="can_focus">True</property>
	      <property name="label" translatable="yes">St_op</property>
	      <property name="use_underline">True</property>
